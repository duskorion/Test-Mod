package com.duskorion.testmod.block;

import net.minecraft.tileentity.TileEntity;
import net.minecraft.world.World;

public class BlockLoremIpsum extends BlockContainerTestMod
{
	 public BlockLoremIpsum()
	 {
		 super();
		 this.setBlockName("loremipsum");
		 this.setBlockTextureName("loremipsum");

	  }
	 
	 @Override
		public TileEntity createNewTileEntity(World world, int meta) {
			return new TileEntityLoremIpsum();
		}
}
