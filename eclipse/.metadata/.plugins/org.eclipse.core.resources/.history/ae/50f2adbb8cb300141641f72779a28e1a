package com.duskorion.testmod.creativetab;

import com.duskorion.testmod.init.ModItems;
import com.duskorion.testmod.reference.Reference;

import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.item.Item;

public class CreativeTabTestMod 
{
	public static final CreativeTabs TESTMOD_TAB = new CreativeTabs(Reference.MOD_ID)
	{
		@Override
		public Item getTabIconItem()
		{
			return ModItems.berry;
		}
		
		@Override
		public String getTranslatedTabLabel()
		{
			return "Test Mod";
		}
	};
}
